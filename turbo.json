{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "dependsOn": ["^db:generate"],
      "cache": false
    },
    "db:build": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^db:generate"],
      "outputs": ["dist/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "dependsOn": ["^db:generate", "^db:push"]
    },
    "start": {
      "dependsOn": [
        "^db:generate",
        "^db:push",
        "^db:generate",
        "^db:build",
        "build"
      ],
      "cache": false
    }
  }
}
